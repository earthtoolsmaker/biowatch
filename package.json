{
  "name": "biowatch",
  "version": "1.0.6",
  "description": "An Electron application with React",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build:python": "cd test-species && uv run build.py",
    "build": "electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --publish always",
    "build:mac": "npm run build && electron-builder --mac --publish always",
    "build:mac:no-sign": "npm run build && electron-builder --mac --publish never --config.mac.identity=null",
    "build:linux": "npm run build && electron-builder --linux --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@headlessui/react": "^2.2.2",
    "@tailwindcss/vite": "^4.0.17",
    "better-sqlite3": "^11.9.1",
    "csv-parser": "^3.2.0",
    "electron-log": "^5.3.0",
    "electron-updater": "^6.3.9",
    "geist": "^1.3.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.486.0",
    "luxon": "^3.6.1",
    "react-error-boundary": "^5.0.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "react-leaflet-heatmap-layer-v3": "^3.0.3-beta-1",
    "react-router": "^7.5.0",
    "recharts": "^2.15.3",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.0.17"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^6.1.0"
  }
}
